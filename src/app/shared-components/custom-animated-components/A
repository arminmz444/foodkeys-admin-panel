.animated-dash-edge {
  fill: none;
  stroke: #888;            /* or your color */
  stroke-width: 2px;
  stroke-dasharray: 6;     /* dash pattern length */
  animation: dashflow 1.5s linear infinite; /* run forever */
}

@keyframes dashflow {
  to {
    stroke-dashoffset: -60; /* negative moves dashes forward */
  }
}
